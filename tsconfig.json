{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "outDir": "dist/",
    "target": "es2016",
    "typeRoots": [
      "node_modules/@types",
      "generated",
    ],
    "resolveJsonModule": true,
    "skipLibCheck": true,
  },
  "include": [
    "generated/**/*",
    "src/**/*",
    "test/**/*"
  ],
  "exclude": [
    "user-generated/**/*"
  ]
}
