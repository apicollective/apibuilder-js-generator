<%_ importDeclarations.forEach(({ defaultExport, namedExports, moduleName }) => { _%>
  <%_ if (defaultExport != null) { _%>
    import <%= defaultExport %> from '<%= moduleName %>';
  <%_ } else if (namedExports != null) { _%>
    import {
      <%_ namedExports.forEach((namedExport) => { _%>
      <%= namedExport %>,
      <%_ }) _%>
    } from '<%= moduleName %>';
  <%_ } else { _%>
    import '<%= moduleName %>';
  <%_ } _%>
<%_ }) _%>