const {
  GraphQLSchema,
  GraphQLObjectType,
  GraphQLNonNull
} = require('graphql');

const schema = new GraphQLSchema({
  query: new GraphQLObjectType({
    name: 'RootQueryType',
    fields: {
    <%_ queries.forEach(({ name, type }) => { _%>
      <%= name %>: {
        type: new GraphQLNonNull(<%= type %>)
      }
    <%_ }) _%>
    }
  }),
});