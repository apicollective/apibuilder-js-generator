{
  label: '<%= helpers.makeLabel(field.name) %>',
  name: '<%= prefix + field.name %>',
  component: 'union-select',
  unions: [<% field.type.types.forEach(function(unionType) { %>
    {
      discriminator: '<%= unionType.discriminatorValue %>',<% if(helpers.getFieldType(unionType.type) === 'modelField') { %>
      fields: [<% unionType.type.fields.forEach(function(modelField) { %><%- include(helpers.getFieldType(modelField.type), {
        field: modelField,
        helpers: helpers,
        prefix: ''
      }); %><%}); %>]
    <% } else { %>fields: [{
      label: 'Type <%= unionType.type %> not supported',
      component: ComponentTypes.PLAIN_TEXT,
    }]<% } %>,
    },<% }); %>],
  <% if(field.isRequired) { %>validate: [
    { type: ValidatorTypes.REQUIRED },
  ],<% } %>
},