{
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "jsRules": {
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "object-literal-sort-keys": false
  },
  "rules": {
    "array-type": false,
    "object-shorthand-properties-first": false
  },
  "defaultSeverity": "error",
  "rulesDirectory": []
}
